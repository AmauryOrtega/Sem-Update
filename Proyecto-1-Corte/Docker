docker run -d -p 49160:22 -p 49161:80 -p 49162:3306 --name=server wnameless/mysql-phpmyadmin

Change /etc/phpmyadmin/apache.conf

	<Directory /usr/share/phpMyAdmin/>
    	order allow,deny
    	allow from all
	</Directory>

Use service apache2 restart

Change /etc/mysql/my.cnf
mysql listen address from 127.0.0.1 to *
